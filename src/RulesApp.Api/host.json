{
    "version": "2.0",
    "logging": {
        "logLevel": {
            "default": "Information",
            "Function": "Information",
            "Host.Triggers.Queue": "Debug"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "extensions": {
        "http": {
            "routePrefix": "",
            "enableBuffering": true
        },
        "queues": {
            "batchSize": 1,
            "newBatchThreshold": 0,
            "maxDequeueCount": 5,
            "maxPollingInterval": "00:00:02",
            "visibilityTimeout": "00:05:00",
            "messageEncoding": "none"
        }
    }
}
